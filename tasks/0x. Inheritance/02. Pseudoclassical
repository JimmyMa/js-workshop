Every Car can honk, but here every Car has a new instance of the honk function! Plz fix! 
--
var Car = function Car () {
  this.honk = function () {
    console.log('Honk!');
  }
};                           

var bmw = new Car();         
bmw.honk();

var honda = new Car();         
honda.honk();

assert(bmw.honk === honda.honk);